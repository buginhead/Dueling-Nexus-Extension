{
  "name": "Dueling Nexus Deck Exporter",
  "version": "1.0",
  "description": "Adds functionality to export decks from Dueling Nexus as ydk files.",
  "permissions": ["https://duelingnexus.com/editor/*", "activeTab", "declarativeContent", "storage"],
  "content_scripts":[{
    "run_at":"document_end",
    "matches": ["https://duelingnexus.com/editor/*"],
    "js":["inject.js"]
  }],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
